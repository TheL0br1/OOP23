<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab2.Controllers.Controller">
    <children>
        <Button layoutX="96.0" layoutY="205.0" mnemonicParsing="false" onAction="#createEntity" prefHeight="32.0" prefWidth="109.0" text="Create new entity" />
        <Group />
        <Button layoutX="96.0" layoutY="298.0" mnemonicParsing="false" onAction="#deleteEntity" text="Delete all entities" />
        <Button layoutX="80.0" layoutY="336.0" mnemonicParsing="false" onAction="#printEntity" text="Print number of entities" />
        <TextField fx:id="damage" accessibleRole="TEXT_AREA" alignment="CENTER" layoutX="76.0" layoutY="64.0" promptText="damage" />
        <TextField fx:id="health" alignment="CENTER" layoutX="76.0" layoutY="96.0" promptText="health" />
        <TextField fx:id="positionX" alignment="CENTER" layoutX="75.0" layoutY="129.0" promptText="position: x" />
        <TextField fx:id="positionY" alignment="CENTER" layoutX="76.0" layoutY="163.0" promptText="posiniton: y" />
        <TextField fx:id="name" alignment="CENTER" layoutX="75.0" layoutY="27.0" promptText="name" />
      <Button layoutX="70.0" layoutY="257.0" mnemonicParsing="false" onAction="#changeActiveAll" text="Change Active of all entities" />
    </children>
</AnchorPane>

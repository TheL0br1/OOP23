<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="433.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.example.kursova.Controllers.Controller">
    <children>
        <Button layoutX="98.0" layoutY="225.0" mnemonicParsing="false" onAction="#createEntity" prefHeight="32.0"
                prefWidth="109.0" text="Create new entity"/>
        <Group/>
        <Button layoutX="96.0" layoutY="320.0" mnemonicParsing="false" onAction="#deleteEntity"
                text="Delete all entities"/>
        <Button layoutX="83.0" layoutY="364.0" mnemonicParsing="false" onAction="#printEntity"
                text="Print number of entities"/>
        <TextField fx:id="damage" accessibleRole="TEXT_AREA" alignment="CENTER" layoutX="78.0" layoutY="84.0"
                   promptText="damage"/>
        <TextField fx:id="health" alignment="CENTER" layoutX="78.0" layoutY="116.0" promptText="health"/>
        <TextField fx:id="positionX" alignment="CENTER" layoutX="77.0" layoutY="149.0" promptText="position: x"/>
        <TextField fx:id="positionY" alignment="CENTER" layoutX="78.0" layoutY="183.0" promptText="posiniton: y"/>
        <TextField fx:id="name" alignment="CENTER" layoutX="77.0" layoutY="47.0" promptText="name"/>
        <Button layoutX="69.0" layoutY="278.0" mnemonicParsing="false" onAction="#changeActiveAll"
                text="Change Active of all entities"/>
        <ChoiceBox fx:id="typeChoice" layoutX="79.0" layoutY="9.0" prefWidth="150.0"/>
    </children>
</AnchorPane>
